[run]
source = .
omit = 
    */tests.py
    */migrations/*
    */settings.py
    */manage.py
    */wsgi.py
    */asgi.py
    */__pycache__/*
    */venv/*
    */env/*
    ../../next/*
    ../../tests/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
